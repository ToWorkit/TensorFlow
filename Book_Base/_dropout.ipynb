    "import tensorflow as tf\n",
    "a = tf.constant([[-1.0, 2.0, 3.0, 4.0]])\n",
    "with tf.Session() as sess:\n",
    "    # noise -> 噪声\n",
    "    b = tf.nn.dropout(a, 0.5, noise_shape = [1, 4])\n",
    "    print(sess.run(b))\n",
    "    c = tf.nn.dropout(a, 0.5, noise_shape = [1, 1])\n",
    "    print(sess.run(c))"
